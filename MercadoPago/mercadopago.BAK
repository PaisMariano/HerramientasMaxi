 _SCREEN.VISIBLE = .F.
 _SCREEN.WINDOWSTATE = 2
 _SCREEN.BACKCOLOR = 16777215
 _SCREEN.CAPTION = "Instalación de MaxiREST"
 _SCREEN.ICON = "MaxiREST.ico"
 _SCREEN.ADDOBJECT("Cuadro", "Image")
 _SCREEN.CUADRO.STRETCH = 0
 _SCREEN.CUADRO.TOP = 10
 _SCREEN.CUADRO.HEIGHT = 300
 _SCREEN.CUADRO.WIDTH = SYSMETRIC(1)-40
 _SCREEN.CUADRO.LEFT = INT((SYSMETRIC(1)-_SCREEN.CUADRO.WIDTH)/2)
 SET SYSMENU OFF
 SET STATUS BAR OFF
 SET SAFETY OFF
 SET TALK OFF
 SET DATE TO FRENCH
 SET CENTURY ON
 _SCREEN.VISIBLE = .T.
 PUBLIC CAD, S_PARAMETRO1
 STORE "" TO CAD, S_PARAMETRO1
 ON ERROR STRTOFILE(DTOC(DATE()) + " " + TIME() + " ERROR Nº: " + STR(ERROR(), 5) + " - " + MESSAGE() + " EN LÍNEA " + ALLT(STR(LINENO())) + " (" + ALLT(PROGRAM()) + ")" + CHR(13) + CHR(10), "c:\ErrInst.LOG", .t.)
 DO FORM mercadopago
 READ EVENTS
 CLEAR EVENTS
 QUIT
ENDPROC